{
    "name": "fadhila36/laravel-indonesian-banks",
    "description": "Comprehensive Indonesian Bank Data for Laravel",
    "keywords": [
        "laravel",
        "indonesia",
        "bank",
        "atm-bersama",
        "finance",
        "indonesian-banks"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Fadhila Abiyyu Faris",
            "email": "fadhilaabiyyu@gmail.com",
            "homepage": "https://fadhilaabiyyu.my.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Fadhila36\\IndonesianBanks\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Fadhila36\\IndonesianBanks\\Providers\\IndonesianBanksServiceProvider"
            ],
            "aliases": {
                "IndonesianBank": "Fadhila36\\IndonesianBanks\\Facades\\IndonesianBank"
            }
        }
    },

    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
